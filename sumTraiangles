vector<int> sumTriangles(const vector<vector<int> >& matrix, int n)
   {
       vector<int> v;
       int sum=0,sum2=0;
   for(int i=0;i<n;i++)
   {
       for(int j=i;j<n;j++)
       {
           sum+=matrix[i][j];
       }
   }
       for(int i=0;i<n;i++)
   {
       for(int j=i;j<n;j++)
       {
           sum2+=matrix[j][i];
       }
   }  
   v.push_back(sum);
   v.push_back(sum2);
   return v;
       // code here
   }
};
